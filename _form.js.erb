var GrabForm = function() {
	var form = $B("form", {
		className: styles["interface"],
		children: [
			this.extractor_id_field	=	$B("input", {type:"hidden",	name:"extractor_id",exclude:true}),
			this.label_field		=	$B("input", {type:"text",	name:"label",		placeholder: "Label Name"}),
			this.xpath_field		=	$B("input", {type:"hidden",	name:"xpath"}),
			this.raw_field			=	$B("input", {type:"hidden",	name:"raw"}),
			this.id_field			=	$B("input",	{type:"hidden",	name:"id",disabled:true}),
			$B("input",{value:"Save",type:"submit",exclude:true})
		],
		action: create_annotation,
		method: "POST"
	});
	Connection.MakeIframeTarget(form,"annotation",function(){
		clearAllInvolvedAndSelector();	
		loader.request({
			domain:	document.location.hostname,
			page:{
				url:	document.location.href,
				title:	document.title
			}
		});
	}
	this.form = form;
};
