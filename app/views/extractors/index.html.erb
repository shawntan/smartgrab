<h1>Listing extractors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @extractors.each do |extractor| %>
  <tr>
    <td><%=h extractor.name %></td>
    <td><%=h extractor.description %></td>
    <td><%= link_to 'Show', extractor %></td>
    <td><%= link_to 'Edit', edit_extractor_path(extractor) %></td>
    <td><%= link_to 'Destroy', extractor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New extractor', new_extractor_path %>