<p>
  <b>Title:</b>
  <%=h @page.title %>
</p>

<p>
  <b>Url:</b>
  <%=h @page.url %>
</p>
<p>
	<b>Labels on this page:</b>
	<table>
		<thead>
			<th>URL</th>
			<th></th>
		</thead>
		<%@page.annotations.each do |annotation|%>
		<tr>
			<td><%=annotation.label%></td>
			<td><%= link_to 'Show',annotation %></td>
		</tr>
		<%end%>
	</table>
</p>

<%= link_to 'Edit', edit_page_path(@page) %> |
<%= link_to 'Back', pages_path %>