<h1>New annotation</h1>

<% form_for(@annotation) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :xpath %><br />
    <%= f.text_field :xpath %>
  </p>
  <p>
    <%= f.label :page_id %><br />
    <%= f.text_field :page_id %>
  </p>
  <p>
    <%= f.label :label %><br />
    <%= f.text_field :label %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', annotations_path %>