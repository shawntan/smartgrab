<h1>Listing page_associations</h1>

<table>
  <tr>
    <th>Page</th>
    <th>Extractor</th>
  </tr>

<% @page_associations.each do |page_association| %>
  <tr>
    <td><%=h page_association.page_id %></td>
    <td><%=h page_association.extractor_id %></td>
    <td><%= link_to 'Show', page_association %></td>
    <td><%= link_to 'Edit', edit_page_association_path(page_association) %></td>
    <td><%= link_to 'Destroy', page_association, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New page_association', new_page_association_path %>