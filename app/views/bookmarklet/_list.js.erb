var GrabList = function(extractor) {
	var list = $B("ul",{className:"xpath_list"});
	var update_list = function(ex) {
		if(!extractor.id) extractor.id = ex.id;
		var annotations = ex.annotations;
		for (var i = 0; i < annotations.length; i++) {
			var li = $B("li",{
				annotation: annotations[i],
				innerHTML: annotations[i].label
			});
			list.appendChild(li);
		}
	}
	var loader = new Connection.JSONDataRequest(
		get_extractor,
		"requestExtractorData",
		update_list
	);
	list.update = function() {
		//console.log(extractor);
		if(extractor.id) loader.request({extractor_id: extractor.id});
		else loader.request(extractor); 
	};
	list.update();
	return list;
};
